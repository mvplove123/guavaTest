<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tyb.dao.UserDao">

	<insert id="insertUser" parameterType="User">
		insert into account(
		user_name,
		age,
		address)
		values
		(
		#{user_name},
		#{age},
		#{address}
	</insert>
</mapper>
